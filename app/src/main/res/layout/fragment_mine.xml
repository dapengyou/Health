<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tv_zxing"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_write_corners"
        android:drawableRight="@mipmap/icon_disclosure_lndicator"
        android:padding="10dp"
        android:text="扫码" />

    <TextView
        android:id="@+id/tv_make_erweima"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_write_corners"
        android:drawableRight="@mipmap/icon_disclosure_lndicator"
        android:padding="10dp"
        android:text="生成二维码" />

    <TextView
        android:id="@+id/androidforjs"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_write_corners"
        android:drawableRight="@mipmap/icon_disclosure_lndicator"
        android:padding="10dp"
        android:text="android 与 js交互" />
</LinearLayout>